{
    "comment": "Houses config for custom components that you'd like to import into Finsemble.",
    "components": {
        "Position Blotter": {
            "window": {
                "url": "$applicationRoot/components/positionblotter/positionblotter.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": "34%",
                "height": "49%"
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "showLinker": true,
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "positionblotter"
                    }
                }
            }
        },
        "Price Blotter": {
            "window": {
                "url": "$applicationRoot/components/priceblotter/priceblotter.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": "49%",
                "height": "49%"
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "showLinker": true,
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "priceblotter"
                    }
                }
            }
        },
        "Trade Blotter": {
            "window": {
                "url": "$applicationRoot/components/tradeblotter/tradeblotter.html",
                "frame": false,
                "resizable": true,
                "autoShow": true,
                "top": "center",
                "left": "center",
                "width": "100%",
                "height": "51%"
            },
            "component": {
                "inject": false
            },
            "foreign": {
                "services": {
                    "dockingService": {
                        "canGroup": true,
                        "isArrangable": true
                    }
                },
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "showLinker": true,
                        "FSBLHeader": true,
                        "persistWindowState": true
                    },
                    "Toolbar": {
                        "iconClass": "tradeblotter"
                    }
                }
            }
        },
        "Advanced Chart": {
            "window": {
                "url": "http://finsemble.chartiq.com/fin/components/premium/chart/sample-template-advanced.html",
                "defaultTop": 200,
                "defaultLeft": 100,
                "defaultWidth": 640,
                "defaultHeight": 480,
                "minWidth": 180,
                "minHeight": 400
            },
            "component": {
                "type": "chart"
            },
            "foreign": {
                "services": {},
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true,
                        "showLinker": true
                    }
                }
            }
        },
        "News": {
            "window": {
                "url": "https://finsemble.chartiq.com/fin/components/premium/news/news.html",
                "top": "center",
                "left": "center",
                "width": "17%",
                "height": "49%"
            },
            "component": {
                "type": "news"
            },
            "foreign": {
                "services": {},
                "components": {
                    "App Launcher": {
                        "launchableByUser": true
                    },
                    "Window Manager": {
                        "FSBLHeader": true,
                        "persistWindowState": true,
                        "showLinker": true
                    }
                }
            }
        }
    }
}
