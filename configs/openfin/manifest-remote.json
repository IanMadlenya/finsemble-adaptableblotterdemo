{
    "devtools_port": 9090,
    "startup_app": {
        "name": "Adaptable Blotter finsemble",
        "url": "http://beta.adaptableblotter.com/finsembledemo/node_modules/@chartiq/finsemble/dist/components/system/serviceManager/serviceManager.html",
        "uuid": "Finsemble",
        "applicationIcon": "http://beta.adaptableblotter.com/finsembledemo/node_modules/@chartiq/finsemble/dist/components/assets/img/Finsemble_Taskbar_Icon.png",
        "defaultTop": 0,
        "defaultLeft": 0,
        "showTaskbarIcon": true,
        "autoShow": false,
        "frame": false,
        "resizable": false,
        "maximizable": false,
        "delay_connection": true,
        "contextMenu": true,
        "cornerRounding": {
            "width": 4,
            "height": 4
        },
        "alwaysOnTop": false
    },
    "runtime": {
        "arguments": "--noerrdialogs  --v=1 --no-sandbox  --enable-crash-reporting",
        "version": "stable"
    },
    "shortcut": {
        "company": "Adaptable Tools",
        "description": "Adaptable Blotter",
        "icon": "http://beta.adaptableblotter.com/finsembledemo/node_modules/@chartiq/finsemble/dist/components/assets/img/Finsemble_Taskbar_Icon.ico",
        "name": "Adaptable Blotter finsemble - Beta",
        "target": [
            "desktop",
            "start-menu"
        ],
        "force": false,
        "startMenuRootFolder": "Adaptable Tools"
    },
    "dialogSettings": {
        "logo": "http://beta.adaptableblotter.com/finsembledemo/node_modules/@chartiq/finsemble/dist/components/assets/img/ciq-banner-100x25.png",
        "bgColor": 4280798349,
        "textColor": 4293521652,
        "progressBarBgColor": 4294967295,
        "progressBarFillColor": 4282684881,
        "progressBarBorderColor": 4293521652
    },
    "supportInformation": {
        "company": "Adaptable Blotter",
        "product": "Adaptable Blotter finsemble Beta",
        "email": "support@adaptabletools.com"
    },
    "fileName": "Adaptable-Blotter-finsemble-beta-installer",
    "appAssets": [
        {
            "src": "http://connect2.chartiq.com/finsemble/Assimilation.zip",
            "version": "1.0.4",
            "alias": "assimilation",
            "target": "assimilationMain.exe"
        }
    ],
    "finsemble": {
        "applicationRoot": "http://beta.adaptableblotter.com/finsembledemo/dist",
        "moduleRoot": "http://beta.adaptableblotter.com/finsembledemo/node_modules/@chartiq/finsemble/dist",
        "importConfig": [
            "$applicationRoot/configs/application/config.json"
        ]
    }
}