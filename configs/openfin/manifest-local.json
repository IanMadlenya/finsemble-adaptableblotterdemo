{
    "devtools_port": 9090,
    "startup_app": {
        "name": "ChartIQ Local",
        "url": "http://localhost:3375/yourSubDirectory/node_modules/@chartiq/finsemble/dist/components/system/serviceManager/serviceManager.html",
        "uuid": "Finsemble",
        "applicationIcon": "http://images8.webydo.com/92/9227175/3958/FB1B6AA0-1649-8F4A-7ED2-1F1B38CCFC1A.png",
        "defaultTop": 0,
        "defaultLeft": 0,
        "showTaskbarIcon": true,
        "autoShow": false,
        "frame": false,
        "resizable": false,
        "maximizable": false,
        "delay_connection": true,
        "contextMenu": true,
        "cornerRounding": {
            "width": 4,
            "height": 4
        },
        "alwaysOnTop": false
    },
    "runtime": {
        "arguments": "--noerrdialogs  --v=1 --no-sandbox  --enable-crash-reporting",
        "version": "7.*"
    },
    "shortcut": {
        "company": "ChartIQ",
        "description": "ChartIQ Local",
        "icon": "http://images8.webydo.com/92/9227175/3958/FB1B6AA0-1649-8F4A-7ED2-1F1B38CCFC1A.png",
        "name": "ChartIQ - LOCAL",
        "target": [
            "desktop",
            "start-menu"
        ],
        "force": false,
        "startMenuRootFolder": "ChartIQ Local"
    },
    "dialogSettings": {
        "logo": "http://localhost:3375/yourSubDirectory/node_modules/@chartiq/finsemble/dist/components/assets/img/ciq-banner-100x25.png",
        "bgColor": 4280798349,
        "textColor": 4293521652,
        "progressBarBgColor": 4294967295,
        "progressBarFillColor": 4282684881,
        "progressBarBorderColor": 4293521652
    },
    "supportInformation": {
        "company": "ChartIQ",
        "product": "Finsemble Local Demo",
        "email": "finsemble@chartiq.com"
    },
    "fileName": "ChartIQ-local-installer",
    "appAssets": [
        {
            "src": "http://connect2.chartiq.com/finsemble/Assimilation.zip",
            "version": "1.0.4",
            "alias": "assimilation",
            "target": "assimilationMain.exe"
        }
    ],
    "finsemble": {
        "applicationRoot": "http://localhost:3375/yourSubDirectory/dist",
        "moduleRoot": "http://localhost:3375/yourSubDirectory/node_modules/@chartiq/finsemble/dist",
        "importConfig": [
            "$applicationRoot/configs/application/config.json"
        ]
    }
}